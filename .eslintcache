ojectDialog, setProjectDialog] = useState('')\n\n  function openDialog() {\n    console.log(projectDialog)\n    return (\n      <Dialog open={projectDialog} onClose={() => setProjectDialog('')}>\n        <DialogTitle onClose={() => setProjectDialog('')}>\n          {projectDialog.title}\n        </DialogTitle>\n        <DialogActions>\n          <Carousel>\n            {projectDialog?.images?.map((image, index) => (\n              <div key={index}>\n                <img src={image.url} />\n                <p className=\"legend\">{image.title}</p>\n              </div>\n            ))}\n          </Carousel>\n        </DialogActions>\n        <DialogContent>{projectDialog.description}</DialogContent>\n      </Dialog>\n    )\n  }\n\n  return (\n    <Grid container className=\"section\">\n      {/* title */}\n      <Grid item className=\"title\">\n        <MyTitle title=\"Portfolio\" />\n      </Grid>\n\n      {/* tabs */}\n      <Grid item>\n        <Tabs\n          value={tabValue}\n          indicatorColor=\"white\"\n          className=\"custom_tabs\"\n          onChange={(event, newValue) => setTabValue(newValue)}\n        >\n          <Tab\n            label=\"All\"\n            value=\"All\"\n            className={\n              tabValue === 'All' ? 'customTabs_item active' : 'customTabs_item'\n            }\n          />\n          {[...new Set(portfolioData.map((item) => item.tag))].map(\n            (tag, index) => (\n              <Tab\n                key={index}\n                label={tag}\n                value={tag}\n                className={\n                  tag === tabValue\n                    ? 'customTabs_item active'\n                    : 'customTabs_item'\n                }\n              />\n            ),\n          )}\n        </Tabs>\n      </Grid>\n\n      {/* Projects */}\n      <Grid item xs={12}>\n        <Grid container spacing={8} justify=\"space-evenly\">\n          {portfolioData.map((project, index) => (\n            <>\n     